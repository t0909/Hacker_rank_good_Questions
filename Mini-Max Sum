void miniMaxSum(vector<int> arr) {
    
    
    long long min = arr[0], max= arr[0], sum = arr[0];
    
    for(int i=1; i<5; i++)
    {
        sum+= arr[i];
        if(max<arr[i])
        max= arr[i];
        
        if(min>arr[i])
        min= arr[i];
        
       
       
    }
    
    
    cout<<sum-max<<" "<<sum-min;
    

}
